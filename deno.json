{
  "name": "@danet/cli",
  "version": "0.9.0",
  "exports": {
    "./bundle.ts": "./bundle.ts",
    "./generate.ts": "./generate.ts",
    "./main.ts": "./main.ts",
    "./deployToDenoDeploy.ts": "./deployToDenoDeploy.ts",
    "./deps.ts": "./deps.ts",
    "./run.ts": "./run.ts",
    "./deno.json": "./deno.json"
  },
  "fmt": {
    "options": {
      "singleQuote": true,
      "useTabs": true
    },
    "files": {
      "exclude": [
        "node_modules/",
        "coverage/",
        "doc/"
      ]
    }
  },
  "tasks": {
    "run": "deno run --allow-read --allow-write --allow-run --allow-env main.ts"
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@ts-morph/ts-morph": "jsr:@ts-morph/ts-morph",
    "@danet/core": "jsr:@danet/core@2"
  }
}
