{
  "name": "@danet/cli",
  "version": "0.9.4",
  "exports": {
    ".": "./main.ts"
  },
  "fmt": {
    "options": {
      "singleQuote": true,
      "useTabs": true
    },
    "files": {
      "exclude": [
        "node_modules/",
        "coverage/",
        "doc/"
      ]
    }
  },
  "tasks": {
    "run": "deno run --allow-read --allow-write --allow-run --allow-env main.ts"
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@ts-morph/ts-morph": "jsr:@ts-morph/ts-morph@23.0.0",
    "@danet/core": "jsr:@danet/core@2"
  },
  "license": "MIT"
}
